programa {
  funcao inicio() {
    inteiro num1, num2, soma, sub, mul, div
    escreva("Digite um número: ")
    leia(num1)
    escreva ("Digite outro número: ")
    leia(num2)
    soma = num1 + num2
    sub = num1 - num2
    mul = num1 * num2
    div = num1 / num2
    escreva ("\n\nO resultado da soma é: ", soma)
    escreva ("\n\nO resultado da subtração é: ", sub)
    escreva ("\n\nO resultado da multiplicação é: ", mul)
    escreva ("\n\nO resultado da divisão é: ", div)

  }
}
